{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9",
        "build": {
          "env": {
            "PIP_EXTRA_INDEX_URL": "https://pypi.org/simple"
          },
          "commands": [
            "pip install fastapi uvicorn"
          ]
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
