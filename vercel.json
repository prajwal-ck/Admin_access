{
    "version": 2,
    "builds": [
        {
            "source": "Html_python.py",
            "use": "@vercel/python",
            "config": {
                "pythonVersion": "3.12"
            }
        }
    ],
    "routes": [
        {
            "source": "/(.*)",
            "destination": "Html_python.py"
        }
    ]
}


{
  "devCommand": "uvicorn Html_python:app",
  "builds": [
    {
      "src": "Html_python.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Html_python.py"
    }
  ]
}
